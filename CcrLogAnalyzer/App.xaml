<Application x:Class="CcrLogAnalyzer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CcrLogAnalyzer">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/BusyIndicator;component/Theme/Default.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/CcrLogAnalyzer;component/Resources/Styles/CommonStyles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/CcrLogAnalyzer;component/Resources/Styles/Views.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
